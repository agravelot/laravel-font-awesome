{
    "name": "jerodev/laravel-font-awesome",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jerodev",
            "email": "jeroen@deviaene.eu"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jerodev\\LaraFontAwesome\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jerodev\\LaraFontAwesome\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jerodev\\LaraFontAwesome\\FontAwesomeServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "5.6.x|5.7.x|5.8.x",
        "fortawesome/font-awesome": "^5.9"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^7|^8",
        "orchestra/testbench": "^3.5"
    }
}
